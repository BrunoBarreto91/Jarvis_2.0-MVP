      ],
      "id": "31951441-bc5f-4f4d-8857-59d902e45542",
      "name": "Notifications_json_builder"
    },
    {
      "parameters": {
        "jsCode": "const aiText = $input.first().json.output || $input.first().json.text || $input.first().json.response?.generations?.[0]?.[0]?.text;\n\ntry {\n    const jsonMatch = aiText.match(/\\{[\\s\\S]*\\}/);\n    if (jsonMatch) {\n        const data = JSON.parse(jsonMatch[0]);\n        return [{\n            json: {\n                userId: 1,\n                title: data.title || \"Nova Tarefa\",\n                contexto: data.contexto || \"Pessoal\",\n                tag: data.tag || \"Geral\",\n                categoria: data.categoria || \"Tarefa\",\n                prioridade: data.prioridade || \"media\",\n                nivel_energia: data.nivel_energia || \"media\",\n                duracao_estimada_minutos: data.duracao_estimada_minutos || 15,\n                status: \"todo\",\n                notas: data.notas || aiText\n            }\n        }];\n    }\n} catch (e) {\n    return [{ json: { error: \"Erro no mapeamento TDAH\", details: e.message } }];\n}"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
